{"format_version":"1.10.0","animation_controllers":{"controller.animation.paon.attack_controller":{"initial_state":"default","states":{"default":{"transitions":[{"peck_attack":"q.property('hfrlc:state') == 'preparing_attack' && q.property('hfrlc:attack_type') == 'peck_attack'"},{"start_fly_attack":"q.property('hfrlc:state') == 'preparing_attack' && q.property('hfrlc:attack_type') == 'fly_attack'"},{"start_indimidation":"q.property('hfrlc:player_nearby') && !q.property('hfrlc:has_target')"}]},"peck_attack":{"on_entry":["/event entity @s hfrlc:attack_start"],"on_exit":["/event entity @s hfrlc:attack_end"],"transitions":[{"default":"q.state_time >= 1.0"}]},"start_fly_attack":{"on_entry":["/event entity @s hfrlc:attack_start"],"transitions":[{"loop_fly_attack":"q.state_time >= 0.79"}]},"loop_fly_attack":{"on_exit":["/event entity @s hfrlc:movement_off"],"transitions":[{"end_fly_attack":"q.property('hfrlc:state') != 'attacking'"}]},"end_fly_attack":{"on_exit":["/event entity @s hfrlc:attack_end","/event entity @s hfrlc:movement_on"],"transitions":[{"default":"q.state_time >= 1.0"}]},"start_indimidation":{"transitions":[{"loop_indimidation":"q.state_time >= 1.0"}]},"loop_indimidation":{"transitions":[{"end_indimidation":"!q.property('hfrlc:player_nearby')"}]},"end_indimidation":{"on_exit":["/event entity @s hfrlc:movement_on"],"transitions":[{"default":"q.state_time >= 0.96"}]}}}}}