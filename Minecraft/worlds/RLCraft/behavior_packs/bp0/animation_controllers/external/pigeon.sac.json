{"format_version":"1.10.0","animation_controllers":{"controller.animation.hfrlc.pigeon.death":{"initial_state":"default","states":{"default":{"transitions":[{"death_ground":"q.property('hfrlc:is_death') && !q.property('hfrlc:flying')"},{"death_fly_start":"q.property('hfrlc:is_death') && q.property('hfrlc:flying')"}]},"death_ground":{"on_entry":["@s hfrlc:start_death"]},"death_fly_start":{"transitions":[{"death_fly_idle":"q.state_time >= 0.6667"}]},"death_fly_idle":{"transitions":[{"death_fly_end":"q.is_on_ground"}]},"death_fly_end":{"on_entry":["@s hfrlc:start_death"]}}},"controller.animation.hfrlc.pigeon.landing":{"initial_state":"default","states":{"default":{"transitions":[{"landing":"q.property('hfrlc:states') == 'landing'"}]},"landing":{"transitions":[{"default":"q.property('hfrlc:is_death')"},{"land":"q.is_on_ground"}]},"land":{"transitions":[{"default":"q.state_time >= 1"}],"on_entry":["@s hfrlc:land"]}}},"controller.animation.hfrlc.pigeon.falling":{"initial_state":"default","states":{"default":{"transitions":[{"not_ground":"!q.is_on_ground && q.property('hfrlc:states') != 'landing' && !q.property('hfrlc:flying') && !q.property('hfrlc:is_death')"}]},"not_ground":{"transitions":[{"default":"q.is_on_ground || q.property('hfrlc:flying') || q.property('hfrlc:is_death') || q.property('hfrlc:states') == 'landing'"},{"start_fly":"(v.posy - q.position(1)) >= 4"}],"on_entry":["v.posy = q.position(1);"]},"start_fly":{"transitions":[{"default":"(1.0)"}],"on_entry":["@s hfrlc:fly"]}}},"controller.animation.hfrlc.pigeon.avoiding_mobs":{"initial_state":"default","states":{"default":{"transitions":[{"avoid":"q.is_avoiding_mobs && !q.property('hfrlc:flying') && !q.property('hfrlc:is_death')"}]},"avoid":{"transitions":[{"default":"!q.is_avoiding_mobs || q.property('hfrlc:flying') || q.property('hfrlc:is_death')"}],"on_entry":["@s hfrlc:fly"]}}}}}